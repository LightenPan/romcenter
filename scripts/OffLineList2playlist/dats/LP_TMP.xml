<dat xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="datas.xsd">
    <configuration>
        <datName>LP_FBA_OL</datName>
        <imFolder>LP_FBA_OL</imFolder>
        <datVersion>20190823</datVersion>
        <system>FB Alpha</system>
        <screenshotsWidth>240</screenshotsWidth>
        <screenshotsHeight>160</screenshotsHeight>
        <infos>
            <title default="false" inNamingOption="true" visible="false" />
            <location default="true" inNamingOption="true" visible="true" />
            <publisher default="true" inNamingOption="true" visible="true" />
            <sourceRom default="true" inNamingOption="true" visible="true" />
            <saveType default="true" inNamingOption="true" visible="true" />
            <romSize default="true" inNamingOption="true" visible="true" />
            <releaseNumber default="true" inNamingOption="true" visible="true" />
            <comment default="true" inNamingOption="true" visible="true" />
            <romCRC default="true" inNamingOption="true" visible="true" />
            <im1CRC default="false" inNamingOption="true" visible="true" />
            <im2CRC default="false" inNamingOption="true" visible="true" />
            <languages default="true" inNamingOption="true" visible="true" />
        </infos>
        <canOpen>
            <extension>.zip</extension>
        </canOpen>
        <newDat>
            <datVersionURL />
            <datURL fileName="Unofficial No-Intro Nintendo Game Boy Advance Chinese.zip" />
            <imURL>http://retrogame.dynamic-dns.net/cdn/RetroGame/imgs/LP_FBA_OL</imURL>
        </newDat>
        <search>
            <to auto="true" default="true" value="location" />
            <to auto="false" default="true" value="romSize">
                <find operation="&lt;" value="4194304">&lt; 32 MBits</find>
                <find operation="=" value="4194304">= 32 MBits</find>
                <find operation="=" value="8388608">= 64 MBits</find>
                <find operation="=" value="16777216">= 128 MBits</find>
                <find operation="=" value="33554432">= 256 MBits</find>
            </to>
            <to auto="true" default="true" value="languages" />
            <to auto="false" default="true" value="saveType">
                <find operation="inNoCase" value="EEPROM">Eeprom</find>
                <find operation="inNoCase" value="FLASH">Flash</find>
                <find operation="inNoCase" value="SRAM">SRam</find>
                <find operation="inNoCase" value="NONE">None</find>
            </to>
            <to auto="true" default="true" value="publisher" />
            <to auto="true" default="true" value="sourceRom" />
        </search>
        <romTitle>%u - %n %e %g</romTitle>
    </configuration>
    <games>
    </games>
    <gui>
        <images height="160" width="520">
            <image height="160" width="250" x="0" y="0" />
            <image height="160" width="240" x="270" y="0" />
        </images>
    </gui>
</dat>